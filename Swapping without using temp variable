#include<stdio.h>
int main()
{
      int a,b,temp;
     
      printf("\nEnter First Number: ");
      scanf("%d", &a);
      printf("\nEnter Second Number: ");
      scanf("%d",&b);
      
       
      // For swapping 
       
     a=a+b;
     b=a-b;
     a=a-b;
      printf("\nFirst Number:%d \nSecond Number:%d",a,b);
      return 0;
}
